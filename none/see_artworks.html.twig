
<div class="card-deck">

    {% for seeArtwork in seeArtworks %}
    <div class="card">
        <img src="https://via.placeholder.com/150x150.png" class="card-img-top" alt="{{ seeArtwork.picture }} ">
        <div class="card-body">
            <h5 class="card-title">
                {{ seeArtwork.name }}
            </h5>
            <p class="card-text">
                {{ seeArtwork.description }}
            </p>
            <p class="card-text">
                <small class="text-muted">
                    <a href="#">
                        {{ seeArtwork.category }}
                    </a>
                </small>
            </p>
            <p class="card-text">
                <small class="text-muted">
                    <a href="#">
                        {{ seeArtwork.date }}
                    </a>
                </small>
            </p>
        </div>
    </div>
    {% endfor %}
</div>
</body>
</html>